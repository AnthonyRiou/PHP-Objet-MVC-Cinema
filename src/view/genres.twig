{% extends "base.twig" %}

{% block title %} Genres {% endblock %}

{% block content %}

    <div id="selection">
        {% for genre in genres %}
            <div id="act">
                {{ genre.Nom_genre }}
                <a href = "genre/{{genre.Id_genre}}"><img src={{genre.photo}} width="250"></a><br>
            </div>
        {% endfor %}
    </div>
    
{% endblock %}
   
